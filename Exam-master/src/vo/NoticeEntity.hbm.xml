<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="vo.NoticeEntity" table="notice" schema="exam" dynamic-update="true" dynamic-insert="true">
        <id name="noticeId" column="notice_id"/>
        <property name="noticeTitle" column="notice_title"/>
        <property name="noticeAbout" column="notice_about"/>
        <property name="publishesTime" column="publishes_time"/>
    </class>
</hibernate-mapping>