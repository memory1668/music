<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="vo.ExamineEntity" table="examine" schema="exam" dynamic-update="true" dynamic-insert="true">
        <id name="exId" column="ex_id"/>
        <property name="stuId" column="stu_id"/>
        <property name="exStatus" column="ex_status"/>
        <property name="departId" column="depart_id"/>
    </class>
</hibernate-mapping>